{
	"name": "Beam",
	"version": "0.1.8",
	"manifest_version": 2,
	"description": "An instant, persistent, text and file transfer tray.",
	"icons": {
		"16": "images/icon-16.png",
		"128": "images/icon-128.png"
	},
	"default_locale": "en",
	"permissions": [
		"webRequest",
		"identity",
		"*://*.google.com/"
	],
	"content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://cdnjs.cloudflare.com https://cdn.firebase.com https://www.googleapis.com; object-src 'self'",
	"oauth2": {
		"client_id": "487313386297-68sb602e6d4knsffmjf9t7n7kin3s2nr.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/userinfo.email",
			"https://www.googleapis.com/auth/userinfo.profile"
		]
	},
	"key": "",
	"browser_action": {
		"default_icon": {
			"19": "images/icon-19.png",
			"38": "images/icon-38.png"
		},
		"default_title": "Beam",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"images/icon-48.png"
	]
}